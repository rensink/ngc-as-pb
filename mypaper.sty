%%%% PS FIGURES

\newcommand{\abb}[3]{% {FIGNAME}{FACTOR}{CAPTION}
    \begin{figure}
    \centerline{
      \includegraphics[scale = #2]{figs/#1.eps}
    }
%    \def\epsfsize##1##2{#2##1}
%    \centerline{\epsfbox{figs/#1.eps}}
    \caption{#3}
    \label{fig:#1}
    \end{figure}
}

\newcommand{\abbl}[4]{% {FIGNAME}{FACTOR}{CAPTION}{LABEL}
    \begin{figure}
    \def\epsfsize##1##2{#2##1}
    \centerline{\epsfbox{figs/#1.eps}}
    \caption{#3}
    \label{#4}\label{fig:#1}
    \end{figure}
}

\newcommand{\abbinline}[2]{% {FIGNAME}{FACTOR}
    \def\epsfsize##1##2{#2##1}
    \centerline{\epsfbox{figs/#1.eps}}
}


